---
import data from '@schemas/indexPage.json';
interface Props {
	title: string;
   description: string;
}

const { title = data.title, description = data.description }: Props = Astro.props;
---

<head>
   <meta charset="UTF-8" />
   <meta name="viewport" content="width=device-width" />
   <link rel="icon" type="image/x-icon" href="/favicon.ico" />
   <meta name="generator" content={Astro.generator} />
   <meta name="description" content={description} />
   <title>{title}</title>
</head>